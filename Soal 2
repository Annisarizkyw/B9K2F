<script>
usn="akukamuu";
pwd="3105arwd";

if(usn.length==8 && usn.replace(/[^A-Z]/g, "").length==0 && 
	 pwd.length==8 && pwd.replace(/[^A-Z]/g, "").length>=1 && pwd.replace(/[^a-z]/g, "").length>=1 && pwd.replace(/[^0-9]/g, "").length>=1 && (pwd.match(/[!@#$%^&*()_+\-=\[\]{};':"\\|,.<>\/?]/g) || []).length==1)
{
    alert("true");
}
else{
    alert("Failed"); 
}
</script>
